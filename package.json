{
  "name": "@wmakeev/xml-to-csv",
  "version": "3.0.0",
  "type": "module",
  "description": "Streaming convert from xml to csv.",
  "keywords": [
    "xlsx",
    "csv",
    "convert",
    "exceljs"
  ],
  "exports": "./build/src/index.js",
  "files": [
    "build/src/"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "lint": "eslint . --fix",
    "cleanup": "rm -rf build/",
    "compile:dev": "tsc",
    "compile": "tsc --build tsconfig.prod.json",
    "build:dev": "npm run lint && npm run compile:dev",
    "build": "npm run cleanup && npm run lint && npm run compile",
    "test": "npm run build && node -r dotenv/config ./build/test/index.test.js",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "release": "np"
  },
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "np": {
    "yarn": false
  },
  "author": "Vitaliy V. Makeev",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmakeev/xml-to-csv"
  },
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node22": "^22",
    "@tsconfig/strictest": "^2.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.mapvalues": "^4.6.9",
    "@types/node": "^22",
    "@types/sax": "^1.2.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@typescript-eslint/typescript-estree": "^8.43.0",
    "auto-changelog": "^2.5.0",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^17.2.2",
    "envalid": "^8.1.0",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "np": "^10.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint-language-service": "^5.0.5",
    "undici": "^6"
  },
  "dependencies": {
    "lodash.groupby": "^4",
    "lodash.mapvalues": "^4",
    "sax": "^1.4.1"
  }
